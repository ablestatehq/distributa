{
    "source": {
        "include": ["."],
        "exclude": ["./node_modules", "./docs", "./test", "./custom-template"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/",
        "readme": "./docs/home.md",
        "template":"custom-template"
    }
}